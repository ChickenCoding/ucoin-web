<% layout('../submodel.ejs') -%>

<h1><%= subtitle %></h1>
<table class="table table-condensed table-striped">
  <tr>
    <th style="width: 20%;">Key ID</th>
    <th style="width: 80%;">Endpoints</th>
  </tr>
  <% for(var i = 0; i < peers.length; i++){ %>
  <% var peer = peers[i] %>
  <tr>
    <td style="font-family: fixed,courier">0x<%= peer.fingerprint.substring(16,40) %></td>
    <td style="font-family: fixed,courier">
      <% for (var j = 0; j < peer.endpoints.length; j++) { %>
        <%= peer.endpoints[j] %><br/>
      <% } %>
    </td>
  </tr>
  <% } %>
</table>
