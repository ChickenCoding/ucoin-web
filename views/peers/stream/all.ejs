<% layout('../submodel.ejs') -%>

<h1><%= subtitle %></h1>
<table class="table table-condensed table-striped">
  <tr>
    <th style="width: 30%;">Key ID</th>
    <th style="width: 70%;">Endpoints</th>
  </tr>
  <% for(var i = 0; i < peers.length; i++){ %>
  <% var peer = peers[i] %>
  <tr>
    <td style="font-family: fixed,courier">0x<%= peer.fingerprint.substring(16,40) %></td>
    <td style="font-family: fixed,courier">
      <% for (var i = 0; i < peer.endpoints.length; i++) { %>
        <%= peer.endpoints[i] %><br/>
      <% } %>
    </td>
  </tr>
  <% } %>
</table>
