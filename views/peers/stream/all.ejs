<% layout('../submodel.ejs') -%>

<h1><%= subtitle %></h1>
<table class="table table-condensed table-striped">
  <tr>
    <th style="width: 20%;">Key ID</th>
    <th style="width: 30%;">DNS</th>
    <th style="width: 20%;">IPv4</th>
    <th style="width: 20%;">IPv6</th>
    <th style="width: 10%;">Port</th>
  </tr>
  <% for(var i = 0; i < peers.length; i++){ %>
  <% var peer = peers[i] %>
  <tr>
    <td style="font-family: fixed,courier">0x<%= peer.fingerprint.substring(0,16) %></td>
    <td style="font-family: fixed,courier"><%= peer.dns ? peer.dns : '' %></td>
    <td style="font-family: fixed,courier"><%= peer.ipv4 ? peer.ipv4 : '' %></td>
    <td style="font-family: fixed,courier"><%= peer.ipv6 ? peer.ipv6 : '' %></td>
    <td style="font-family: fixed,courier"><%= peer.port ? peer.port : '' %></td>
  </tr>
  <% } %>
</table>
